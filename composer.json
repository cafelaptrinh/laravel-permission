{
    "name": "cafelaptrinh/laravel-role",
    "description": "custom laravel-permission for spatie",
    "keywords": [
        "cafelaptrinh",
        "laravel",
        "permission",
        "permissions",
        "roles",
        "acl",
        "rbac",
        "security",
        "cafelt"
    ],
    "homepage": "https://github.com/cafelaptrinh/laravel-role",
    "license": "MIT",
    "authors": [
        {
            "name": "cafelaptrinh",
            "email": "cafelaptrinh2@gmail.com",
            "homepage": "https://cafelaptrinh.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2.5",
        "illuminate/auth": "^5.8|^6.0|^7.0",
        "illuminate/container": "^5.8|^6.0|^7.0",
        "illuminate/contracts": "^5.8|^6.0|^7.0",
        "illuminate/database": "^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "CafeLT\\Permission\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CafeLT\\Permission\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CafeLT\\Permission\\PermissionServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
